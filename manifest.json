{
    "manifest_version": 3,
    "name": "GoodDealFinder",
    "description": "Finds the actual price of technology on ebay so an optimum deal can be found.",
    "version": "1.3",
    
    "action": {
        "default_popup": "popup.html"
    },
    
    "content_scripts": [{
        "matches": ["https://www.ebay.co.uk/*"],
        "js": ["price_lookup.js", "main.js"],
        "css": ["main.css"]
    }]
  }